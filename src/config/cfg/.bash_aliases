#for ros

alias cake='cd ~/catkin && catkin_make'
alias ck='cd ~/catkin'

alias ros='roscore'
alias topic='rostopic'
alias rviz='rosrun rviz rviz'

alias base='roslaunch base base.launch'
alias laser='roslaunch laser laser.launch'
alias loc='roslaunch localization localization.launch'
alias nav='roslaunch we_navigation navigation.launch'
alias tel='roslaunch joy_control we_joy_control_beitong.launch'
alias net='roslaunch socket socket.launch'
alias audio='roslaunch audio audio.launch'
alias mod='chmod +x'

alias qt='qtcreator'

